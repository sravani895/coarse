<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @student.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student.email %>
</p>

<p>
  <strong>Major:</strong>
  <%= @student.major %>
</p>
<h2>Select Courses</h2>
<ol>
<% @student.courses.each do |course| %>
	<li><%= link_to course.name, course_path(course.id) %> </li>
<% end %>
</ol>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
